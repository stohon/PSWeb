<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="console.aspx" />
            </files>
        </defaultDocument>
    </system.webServer>
    <system.web>
        <customErrors mode="Off"/>
    </system.web>
    <appSettings>
        <add key="PSConsoleShareName" value="PSConsole" />
        <add key="PSConsoleShareLocation" value="\\stonas\Usb\PSConsole" />
        <add key="ReplaceAppRootEnvVarInPSScriptWithActual" value="$env:PSConsole" />
        <add key="AllowUsernameList" value="" />
        <add key="LogPath" value="\\stonas\Usb\PSConsole\logs" />
    </appSettings>
</configuration>